{\n  \"dashboard\": {\n    \"title\": \"NEXUS - Deployment Monitoring\",\n    \"description\": \"Real-time monitoring dashboard for NEXUS deployment and performance metrics\",\n    \"tags\": [\"nexus\", \"deployment\", \"monitoring\"],\n    \"timezone\": \"UTC\",\n    \"panels\": [\n      {\n        \"id\": 1,\n        \"title\": \"Deployment Success Rate\",\n        \"type\": \"gauge\",\n        \"targets\": [\n          {\n            \"expr\": \"nexus_deployment_success_rate\",\n            \"legendFormat\": \"{{ type }}\"\n          }\n        ],\n        \"fieldConfig\": {\n          \"defaults\": {\n            \"unit\": \"percent\",\n            \"min\": 0,\n            \"max\": 100,\n            \"thresholds\": {\n              \"mode\": \"absolute\",\n              \"steps\": [\n                { \"color\": \"red\", \"value\": null },\n                { \"color\": \"yellow\", \"value\": 80 },\n                { \"color\": \"green\", \"value\": 95 }\n              ]\n            }\n          }\n        }\n      },\n      {\n        \"id\": 2,\n        \"title\": \"Deployment Duration\",\n        \"type\": \"graph\",\n        \"targets\": [\n          {\n            \"expr\": \"nexus_deployment_duration_seconds\",\n            \"legendFormat\": \"{{ type }}\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"Duration (seconds)\",\n            \"format\": \"short\"\n          }\n        ]\n      },\n      {\n        \"id\": 3,\n        \"title\": \"Gas Price\",\n        \"type\": \"graph\",\n        \"targets\": [\n          {\n            \"expr\": \"nexus_gas_price_gwei\",\n            \"legendFormat\": \"Gas Price\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"Price (Gwei)\",\n            \"format\": \"short\"\n          }\n        ]\n      },\n      {\n        \"id\": 4,\n        \"title\": \"Gas Estimation Duration\",\n        \"type\": \"histogram\",\n        \"targets\": [\n          {\n            \"expr\": \"nexus_gas_estimation_duration_ms\",\n            \"legendFormat\": \"Duration\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"Duration (ms)\",\n            \"format\": \"short\"\n          }\n        ]\n      },\n      {\n        \"id\": 5,\n        \"title\": \"Transaction Confirmation Rate\",\n        \"type\": \"gauge\",\n        \"targets\": [\n          {\n            \"expr\": \"nexus_transaction_confirmation_rate\",\n            \"legendFormat\": \"Confirmation Rate\"\n          }\n        ],\n        \"fieldConfig\": {\n          \"defaults\": {\n            \"unit\": \"percent\",\n            \"min\": 0,\n            \"max\": 100,\n            \"thresholds\": {\n              \"mode\": \"absolute\",\n              \"steps\": [\n                { \"color\": \"red\", \"value\": null },\n                { \"color\": \"yellow\", \"value\": 85 },\n                { \"color\": \"green\", \"value\": 95 }\n              ]\n            }\n          }\n        }\n      },\n      {\n        \"id\": 6,\n        \"title\": \"Transaction Confirmation Time\",\n        \"type\": \"graph\",\n        \"targets\": [\n          {\n            \"expr\": \"nexus_transaction_confirmation_time_seconds\",\n            \"legendFormat\": \"Confirmation Time\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"Time (seconds)\",\n            \"format\": \"short\"\n          }\n        ]\n      },\n      {\n        \"id\": 7,\n        \"title\": \"Alerts by Level\",\n        \"type\": \"piechart\",\n        \"targets\": [\n          {\n            \"expr\": \"increase(nexus_alerts_total[1h])\",\n            \"legendFormat\": \"{{ level }}\"\n          }\n        ]\n      },\n      {\n        \"id\": 8,\n        \"title\": \"Alert Response Time\",\n        \"type\": \"graph\",\n        \"targets\": [\n          {\n            \"expr\": \"nexus_alert_response_time_seconds\",\n            \"legendFormat\": \"Response Time\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"Time (seconds)\",\n            \"format\": \"short\"\n          }\n        ]\n      },\n      {\n        \"id\": 9,\n        \"title\": \"API Request Duration\",\n        \"type\": \"graph\",\n        \"targets\": [\n          {\n            \"expr\": \"nexus_api_request_duration_ms\",\n            \"legendFormat\": \"{{ method }} {{ endpoint }}\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"Duration (ms)\",\n            \"format\": \"short\"\n          }\n        ]\n      },\n      {\n        \"id\": 10,\n        \"title\": \"API Error Rate\",\n        \"type\": \"graph\",\n        \"targets\": [\n          {\n            \"expr\": \"rate(nexus_api_errors_total[5m])\",\n            \"legendFormat\": \"{{ method }} {{ endpoint }}\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"Errors/sec\",\n            \"format\": \"short\"\n          }\n        ]\n      },\n      {\n        \"id\": 11,\n        \"title\": \"Deployment Failures\",\n        \"type\": \"table\",\n        \"targets\": [\n          {\n            \"expr\": \"increase(nexus_deployment_failures_total[1h])\",\n            \"legendFormat\": \"{{ reason }}\"\n          }\n        ]\n      },\n      {\n        \"id\": 12,\n        \"title\": \"System CPU Usage\",\n        \"type\": \"graph\",\n        \"targets\": [\n          {\n            \"expr\": \"1 - avg by (instance) (rate(node_cpu_seconds_total{mode=\\\"idle\\\"}[5m]))\",\n            \"legendFormat\": \"{{ instance }}\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"CPU Usage\",\n            \"format\": \"percentunit\"\n          }\n        ]\n      },\n      {\n        \"id\": 13,\n        \"title\": \"System Memory Usage\",\n        \"type\": \"graph\",\n        \"targets\": [\n          {\n            \"expr\": \"1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)\",\n            \"legendFormat\": \"{{ instance }}\"\n          }\n        ],\n        \"yaxes\": [\n          {\n            \"label\": \"Memory Usage\",\n            \"format\": \"percentunit\"\n          }\n        ]\n      }\n    ],\n    \"refresh\": \"30s\",\n    \"time\": {\n      \"from\": \"now-1h\",\n      \"to\": \"now\"\n    },\n    \"timepicker\": {\n      \"refresh_intervals\": [\"10s\", \"30s\", \"1m\", \"5m\", \"15m\", \"30m\", \"1h\"]\n    }\n  }\n}\n
